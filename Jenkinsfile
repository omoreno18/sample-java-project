pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        echo 'building la aplicacion'
      }
    }
    stage("test") {
      steps {
        echo 'testing la aplicacion'
      }
    }
    stage("deploy") {
      steps {
        echo 'deploying la aplicacion'
      }
    }
  }  
}

